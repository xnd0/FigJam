<div class="menu-container">
  <div class="card-header col-md-12 text-center">
    <h2>
      FigJam 
    </h2>
  </div>

  <!-- This is the form for adding a new dish -->
  <div class="add-dish-container">
    <h2>
      What are you listening to right now?
    </h2>
    <form class="form login-form">
      <label for="song_name">
        Song Name:
      </label>
      <br />
      <textarea id="song_name" name="song_name" rows="2" cols="30">

    </textarea>
      <br />
      <label for="description">
        Description:
      </label>
      <br />
      <textarea id="description" name="description" rows="5" cols="30">

    </textarea>
      <br />
      <label for="username">
        User Name:
      </label>
      <br />
      <textarea id="user_name" name="user_name" rows="2" cols="30">

    </textarea>
      <br />
      <br />
      <button type="submit" value="Submit">
        Submit
      </button>
    </form>
  </div>
  {{#each songs as |song|}}
<div>
  <p>{{song.song_name}}</p>
  <p>{{song.artist_name}}</p>

</div>
{{/each}}
</div>


<!--unsure what file to link to. I'm assuming js file-->
<script src="./js/profile.js"></script>
